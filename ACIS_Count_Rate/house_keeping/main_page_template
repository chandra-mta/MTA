<script>
    function WindowOpener(imgname) {
        msgWindow = open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=no,status=no,,width=1000,height=600,resize=no");
        msgWindow.document.clear();
        msgWindow.document.write("<html><title>ACIS Count Rate:   "+imgname+"</TITLE>");
        msgWindow.document.write("<body>");
        msgWindow.document.write("<img src='./Plots/"+imgname+"' border =0 style=\"width:95%;\"><p></p></body></html>")
        msgWindow.document.close();
        msgWindow.focus();
    }
</script>
