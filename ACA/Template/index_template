<head>
    <title>ACA Trending</title>
    <script language="JavaScript">
        function WindowOpener(imgname, plttitle) {
            msgWindow = open("","displayname","toolbar=no,directories=no,menubar=yes,location=no,scrollbars=no,status=no,width=680,height=400,resize=no");
            msgWindow.document.close();
            msgWindow.document.open();
            msgWindow.document.write("<html><title>Monitor plot:   "+imgname+"</title>");
            msgWindow.document.write("<body bgcolor='white'>");
            msgWindow.document.write("<center>");
            msgWindow.document.write("<h3>"+plttitle+"</h3>");
            msgWindow.document.write("</center>");
            msgWindow.document.write("<img src='"+imgname+"' border=0 width=640 height=320><P></body></html>")
            msgWindow.document.close();
            msgWindow.focus(); 
        }
        function ViewPlots(imgnames, plttitles) {
            pltWindow = open("","mag_i_avgs","scrollbars=yes,menubar=yes,width=680,height=640");
            pltWindow.document.close();
            pltWindow.document.open();
            pltWindow.document.write(" <html><head><title>Monitor plots</title></head> ");
            pltWindow.document.write("<body bgcolor='white'>");
            pltWindow.document.write("<center>");
            for(var ii=0; ii<imgnames.length;ii++) {
                pltWindow.document.write("<h3>"+plttitles[ii]+"</h3>");
                pltWindow.document.write("<img src='"+imgnames[ii]+"' border=0 width=640 height=320><P>");
            }
            pltWindow.document.write("</center>");
            pltWindow.document.write("</body></html>");
            pltWindow.document.close(); 
            pltWindow.focus(); 
        }
    </script>
    <style>
         td{text-align:center;}
        
        th.rotate{writing-mode: vertical-lr; text-orientation: upright;}
        span.rotate{
            writing-mode: vertical-lr; 
            text-orientation: upright;
            padding-right:20px;
        }
    </style>


</head>
